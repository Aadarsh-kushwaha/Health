    <!-- Emergency SOS Modal -->
    <div class="emergency-modal" id="emergencyModal">
        <div class="modal-content emergency-content">
            <div class="modal-header">
                <h3><i class="fas fa-exclamation-triangle"></i> Emergency SOS</h3>
                <button class="modal-close" id="emergencyClose">
                    <i class="fas fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <div class="emergency-info">
                    <p><strong>Are you experiencing a medical emergency?</strong></p>
                    <p>If this is a life-threatening emergency, please call emergency services immediately:</p>
                    <div class="emergency-numbers">
                        <div class="emergency-number">
                            <i class="fas fa-phone"></i>
                            <span>108 - Emergency Services</span>
                            <button class="btn-primary" onclick="window.open('tel:108')">Call Now</button>
                        </div>
                        <div class="emergency-number">
                            <i class="fas fa-ambulance"></i>
                            <span>102 - Ambulance</span>
                            <button class="btn-primary" onclick="window.open('tel:102')">Call Now</button>
                        </div>
                    </div>
                    <div class="emergency-actions">
                        <button class="btn-secondary" id="emergencyCancel">Not an Emergency</button>
                        <button class="btn-primary" id="emergencyContact">Contact Nearest Doctor</button>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- Notification Toast -->
    <div class="notification-toast" id="notificationToast">
        <div class="toast-content">
            <i class="fas fa-bell"></i>
            <span class="toast-message">New appointment confirmed!</span>
        </div>
    </div>

