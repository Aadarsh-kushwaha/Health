                    <label>Consultation Fee</label>
                    <select id="feeFilter">
                        <option value="">Any Fee</option>
                        <option value="0-500">₹0-500</option>
                        <option value="500-1000">₹500-1000</option>
                        <option value="1000+">₹1000+</option>
                    </select>
                </div>
            </div>
            <div class="search-results" id="searchResults">
                <!-- Search results will be populated here -->
            </div>
        </div>
    </div>

    <!-- User Dashboard Modal -->
    <div class="dashboard-modal" id="dashboardModal">
        <div class="dashboard-content">
            <div class="dashboard-header">
                <h3>My Health Dashboard</h3>
                <button class="dashboard-close" id="dashboardClose">
                    <i class="fas fa-times"></i>
                </button>
            </div>
            <div class="dashboard-tabs">
                <button class="tab-btn active" data-tab="overview">Overview</button>
                <button class="tab-btn" data-tab="appointments">Appointments</button>
                <button class="tab-btn" data-tab="prescriptions">Prescriptions</button>
                <button class="tab-btn" data-tab="reports">Reports</button>
            </div>
            <div class="dashboard-body">
                <div class="tab-content active" id="overview">
                    <div class="health-metrics">
                        <div class="metric-card">
                            <h4>Health Score</h4>
                            <div class="metric-value">85%</div>
                            <div class="metric-progress">
                                <div class="progress-bar" style="width: 85%"></div>
                            </div>
                        </div>
                        <div class="metric-card">
                            <h4>Active Medications</h4>
                            <div class="metric-value">3</div>
                        </div>
                        <div class="metric-card">
                            <h4>Upcoming Appointments</h4>
                            <div class="metric-value">2</div>
                        </div>
                    </div>
                </div>
                <div class="tab-content" id="appointments">
                    <div class="appointment-list">
                        <div class="appointment-item">
                            <div class="appointment-info">
                                <h5>Dr. Sarah Johnson</h5>
                                <p>Cardiologist</p>
                                <span class="appointment-date">Tomorrow, 10:00 AM</span>
                            </div>
                            <div class="appointment-actions">
                                <button class="btn-secondary">Reschedule</button>
                                <button class="btn-primary">Join Call</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- AI Symptom Checker Modal -->
    <div class="symptom-modal" id="symptomModal">
        <div class="modal-content">
            <div class="modal-header">
                <h3>AI Symptom Checker</h3>
                <button class="modal-close" id="symptomClose">
                    <i class="fas fa-times"></i>
                </button>
            </div>
            <div class="modal-body">
                <div class="symptom-input">
                    <textarea id="symptomText" placeholder="Enter your symptoms in detail... (e.g., headache, fever, nausea, etc.)" rows="4"></textarea>
                </div>
                <div class="symptom-suggestions">
                    <h4>Common Symptoms:</h4>
                    <div class="symptom-tags">
                        <span class="symptom-tag" data-symptom="headache">Headache</span>
                        <span class="symptom-tag" data-symptom="fever">Fever</span>
                        <span class="symptom-tag" data-symptom="cough">Cough</span>
                        <span class="symptom-tag" data-symptom="nausea">Nausea</span>
                        <span class="symptom-tag" data-symptom="fatigue">Fatigue</span>
                        <span class="symptom-tag" data-symptom="chest pain">Chest Pain</span>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button class="btn-secondary" id="symptomCancel">Cancel</button>
                <button class="btn-primary" id="symptomCheck">Check Symptoms</button>
            </div>
        </div>
    </div>

